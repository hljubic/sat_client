{
  "name": "Student Activity Tracker",
  "description": "Google Chrome dodatak za praÄ‡enje aktivnosti studenata za vrijeme rada online testova.",
  "version": null,
  "permissions": [
    "https://*/",
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "icons": {
    "16": "icons/fpmoz_16.png",
    "48": "icons/fpmoz_48.png",
    "128": "icons/fpmoz_128.png"
  },
  "browser_action": {
    "default_title": "fpmoz-tracker",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["tracker.js"]
    }
  ]
}
